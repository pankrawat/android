<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ececec">

    <GridView
        android:id="@+id/gridview1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:numColumns="2"
        android:verticalSpacing="10dp"
        android:horizontalSpacing="15dp"
        android:gravity="center"
android:layout_marginLeft="@dimen/marginSmall"
        android:layout_marginRight="@dimen/marginSmall"
        android:layout_marginTop="@dimen/dp_14"
        android:layout_marginBottom="@dimen/marginSmall"
        android:stretchMode="columnWidth" android:visibility="visible">

    </GridView>

    <TextView android:id="@+id/txtError" android:layout_width="wrap_content"
        android:layout_height="wrap_content" android:text="No Seller Contact"
        android:layout_centerInParent="true"
        android:textSize="@dimen/sp_18"
        android:textColor="@color/overlay"
        android:visibility="gone"

        />

</RelativeLayout>
